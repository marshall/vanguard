{
  "debug": false,
  "work_dir": "/var/run/vanguard",

  "programs": {
    "direwolf": "/usr/local/bin/direwolf",
    "gpsd": "/usr/sbin/gpsd",
    "streamer": "/usr/bin/streamer"
  },

  "gps": {
    "device": "/dev/ttyO1",
    "gpsd_sock": "/var/run/gpsd.sock",
    "uart": "UART1"
  },

  "camera": {
    "device": "/dev/video0",
    "resolution": "1280x960",
    "quality": 90,
    "depth": 24,
    "interval": 15
  },

  "radios": {
    "primary": {
      "type": "xtend900",
      "protocol": "vanguard",
      "uart": "UART5",
      "device": "/dev/ttyO5",
      "baudrate": 9600
    },

    "secondary": {
      "type": "tx2h",
      "protocol": "aprs",
      "device": "/tmp/kisstnc",
      "baudrate": 1200,
      "callsign": "N5JHH-2",
      "ptt_pin_num": 61,
      "ptt_pin": "P8_26",
      "ptt_high": 3.0
    }
  },

  "beacon": {
    "interval": 15
  },

  "telemetry": {
    "int_temp_pin": "P9_37",
    "ext_temp_pin": "P9_39",
    "interval": 15
  }
}
